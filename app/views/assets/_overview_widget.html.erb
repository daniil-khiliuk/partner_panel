                                <div class="col-auto g-0">
                                    <img src=<%= image_url('account-icon.png') %> width="24" height="24">
                                </div>

                                <div class="col-auto g-0 ms-2 mt-1">
                                    <b>0</b>
                                </div>
                            </div>
                        </div>
                        
